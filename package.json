{
  "name": "single-spa-vue-ts",
  "version": "0.1.0-alpha.5",
  "description": "a single-spa plugin for vue.js applications",
  "type": "module",
  "main": "dist/single-spa-vue.umd.js",
  "module": "dist/single-spa-vue.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/single-spa-vue.esm.js"
    }
  },
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "build:watch": "rollup -cw",
    "test": "cross-env BABEL_ENV=test vitest run --coverage",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AckermannJan/single-spa-vue"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "single-spa",
    "vue",
    "single",
    "page",
    "app",
    "spa"
  ],
  "author": "single-spa core team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AckermannJan/single-spa-vue/issues"
  },
  "homepage": "https://github.com/AckermannJan/single-spa-vue#readme",
  "peerDependencies": {
    "vue": "^2.7.0 || >=3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.25.3",
    "@eslint/js": "^9.8.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/eslint__js": "^8.42.3",
    "@vitest/coverage-v8": "^2.0.5",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css.escape": "^1.5.1",
    "eslint": "^9.8.0",
    "husky": "^9.1.4",
    "jsdom": "^24.1.1",
    "prettier": "^3.3.3",
    "pretty-quick": "^4.0.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.20.0",
    "single-spa": "^6.0.1",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.1",
    "vitest": "^2.0.5",
    "vue": "^3.4.23",
    "vue-tsc": "^2.0.13",
    "vue2": "npm:vue@2.7.16"
  },
  "dependencies": {
    "vue-demi": "^0.14.10"
  }
}
